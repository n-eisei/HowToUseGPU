Host sunylab-gpususer
    HostName lab.sunyai.com
    User gpususer
    IdentityFile ~/.ssh/gpususer.pem

Host gws-gpususer
    HostName www.sunyai.com
    User gpususer
    IdentityFile ~/.ssh/gpususer.pem
    ProxyCommand  ssh -W %h:%p sunylab-gpususer

Host gpu-server
    HostName localhost
    ProxyCommand  ssh -W %h:%p gws-gpususer
